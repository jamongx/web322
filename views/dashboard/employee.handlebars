<div class="flex-dash">
    <div>
        <h3>{{user.firstName}} {{user.lastName}}</h3></br>
        <h3>Cuisine Package List</h3></br>
    </div>
    <table>
        <thead>
            <th>Image</th>
            <th>Rank</th>
            <th>Country</th>
            <th>Meals</th>
            <th>Name</th>
            <th>Price</th>
        </thead>
        <tbody>
            {{#each cuisines }}
            <tr>
                <td><img src="{{this.image}}" style="width:80px;height:auto;border:0;" ></img></td>
                <td>{{this.rank}}</td>
                <td>{{this.country}}</td>
                <td>{{this.meals}}</td>
                <td><a href='/dashboard/read?_id={{this._id}}'>{{this.name}}</a></td>
                <td>{{this.price}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
    <form action="/dashboard/create">
        <input type="submit" value="Create Package">
    </form>
</div>